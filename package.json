{
  "name": "print-message",
  "version": "0.0.0-semantic-release",
  "description": "NodeJS module for printing out beautiful messages in console",
  "main": "lib/print.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "keywords": [
    "print-message",
    "print",
    "log",
    "message",
    "border",
    "console",
    "beautiful",
    "nice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/node-print-message.git"
  },
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://github.com/ghaiklor"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghaiklor/node-print-message/issues"
  },
  "homepage": "https://github.com/ghaiklor/node-print-message",
  "dependencies": {
    "chalk": "2.4.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-add-module-exports": "0.3.3",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.1",
    "coveralls": "3.0.1",
    "cz-conventional-changelog": "2.1.0",
    "isparta": "4.1.1",
    "mocha": "5.0.0",
    "semantic-release": "15.13.2",
    "sinon": "6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}
