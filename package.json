{
  "name": "print-message",
  "description": "NodeJS module for printing out beautiful messages in console",
  "main": "index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "keywords": [
    "print-message",
    "print",
    "log",
    "message",
    "border",
    "console",
    "beautiful",
    "nice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/node-print-message.git"
  },
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://github.com/ghaiklor"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghaiklor/node-print-message/issues"
  },
  "homepage": "https://github.com/ghaiklor/node-print-message",
  "dependencies": {
    "chalk": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.2.0",
    "babel-preset-es2015-node4": "2.0.1",
    "chai": "3.4.1",
    "coveralls": "2.11.4",
    "cz-conventional-changelog": "1.1.4",
    "isparta": "4.0.0",
    "mocha": "2.3.4",
    "semantic-release": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
